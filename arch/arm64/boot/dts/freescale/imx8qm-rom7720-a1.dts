&iomuxc {
	imx8qm-arm2 {
		pinctrl_gpio_power: gpio_power_grp{
			fsl,pins = <
				SC_P_SCU_GPIO0_02_LSIO_GPIO0_IO30		0x00000021
				SC_P_SCU_GPIO0_03_LSIO_GPIO0_IO31		0x00000021
				SC_P_SCU_GPIO0_06_LSIO_GPIO1_IO02		0x00000021
			>;
		};

		pinctrl_gpio_sleep: gpio_sleep_grp{
			fsl,pins = <
				SC_P_SCU_GPIO0_05_LSIO_GPIO1_IO01		0x00000021
			>;
		};

		pinctrl_common_gpio: common_gpio_grp{
			fsl,pins = <
				SC_P_M40_GPIO0_00_LSIO_GPIO0_IO08		0x00000021	/* GPIO0 */
				SC_P_M40_GPIO0_01_LSIO_GPIO0_IO09		0x00000021	/* GPIO1 */
				SC_P_FLEXCAN2_RX_LSIO_GPIO4_IO01		0x00000021	/* GPIO2 */
				SC_P_FLEXCAN2_TX_LSIO_GPIO4_IO02		0x00000021	/* GPIO3 */
				SC_P_ENET1_RGMII_TXC_LSIO_GPIO6_IO10	0x00000021	/* GPIO4 */
				SC_P_ENET1_RGMII_TX_CTL_LSIO_GPIO6_IO11	0x00000021	/* GPIO5 */
				SC_P_ENET1_RGMII_TXD0_LSIO_GPIO6_IO12	0x00000021	/* GPIO6 */
				SC_P_ENET1_RGMII_TXD1_LSIO_GPIO6_IO13	0x00000021	/* GPIO7 */
			>;
		};

		pinctrl_gpio_batlow: gpio_batlow_grp{
			fsl,pins = <
				SC_P_M41_GPIO0_00_LSIO_GPIO0_IO12		0x00000021
			>;
		};

		pinctrl_gpio_thrm: gpio_thrm_grp{
			fsl,pins = <
				SC_P_M41_GPIO0_01_LSIO_GPIO0_IO13		0x00000021
				SC_P_SIM0_GPIO0_00_LSIO_GPIO0_IO05		0x00000021
			>;
		};

		pinctrl_gpio_usb_oc: gpio_usb_oc_grp{
			fsl,pins = <
				SC_P_SIM0_CLK_LSIO_GPIO0_IO00		0x00000021
				SC_P_SIM0_IO_LSIO_GPIO0_IO02		0x00000021
				SC_P_SIM0_RST_LSIO_GPIO0_IO01		0x00000021
			>;
		};

		pinctrl_gpio_lvds0: gpio_lvds0_grp {
			fsl,pins = <
				SC_P_LVDS1_GPIO00_LSIO_GPIO1_IO10	0x00000021	
				SC_P_LVDS0_GPIO01_LSIO_GPIO1_IO05	0x00000021
			>;
		};

		pinctrl_gpio_watchdog: gpio_watchdog_grp{
			fsl,pins = <
				SC_P_FLEXCAN0_TX_LSIO_GPIO3_IO30		0x00000021
				SC_P_FLEXCAN0_RX_LSIO_GPIO3_IO29		0x00000021
			>;
		};

		pinctrl_gpio_i2s_rst: gpio_i2s_rst{
			fsl,pins = <
				SC_P_SPI3_CS0_LSIO_GPIO2_IO20		0x00000021
			>;
		};
/*
		pinctrl_fan: pwm_fanout_grp{
			fsl,pins = <
				SC_P_GPT1_COMPARE_LSIO_PWM2_OUT		0x00000021
			>;
		};
*/
	};
};

